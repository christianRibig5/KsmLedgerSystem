<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="600.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ksmledger.frames.DrawerContentController">
   <children>
      <ImageView fitHeight="200.0" fitWidth="201.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/milky-way-2695569_960_720.jpg" />
         </image>
      </ImageView>
      <Button fx:id="registerMember" accessibleText="Material_One" mnemonicParsing="false" onAction="#registerMemberAction" prefHeight="27.0" prefWidth="200.0" styleClass="button:hover" stylesheets="@../../../style.css" text="Register  Member" />
      <Button fx:id="recordDues" accessibleText="Material_One" layoutX="10.0" layoutY="123.0" mnemonicParsing="false" onAction="#recordDuesAction" prefHeight="27.0" prefWidth="200.0" styleClass="button:hover" stylesheets="@../../../style.css" text="Record Dues " />
      <Button fx:id="recordPayment" accessibleText="Material_Two" layoutX="10.0" layoutY="121.0" mnemonicParsing="false" onAction="#recordPaymentAction" prefHeight="27.0" prefWidth="200.0" styleClass="button:hover" stylesheets="@../../../style.css" text="Record Payment" />
      <Button fx:id="viewMembers" accessibleText="Material_Three" layoutX="10.0" layoutY="148.0" mnemonicParsing="false" onAction="#viewMemberAction" prefHeight="27.0" prefWidth="200.0" styleClass="button:hover" stylesheets="@../../../style.css" text="Members List" />
      <Button fx:id="ksmLedger" accessibleText="Materia_Four" layoutX="10.0" layoutY="148.0" mnemonicParsing="false" onAction="#ledgerAction" prefHeight="0.0" prefWidth="200.0" styleClass="button:hover" stylesheets="@../../../style.css" text="KSM Ledger" />
      <Button fx:id="debitLedger" accessibleText="Material_Five" layoutX="10.0" layoutY="177.0" mnemonicParsing="false" onAction="#debitAction" prefHeight="27.0" prefWidth="200.0" styleClass="button:hover" stylesheets="@../../../style.css" text="Debit Ledger" />
      <Button fx:id="exit" accessibleText="Materia_Six" layoutX="10.0" layoutY="231.0" mnemonicParsing="false" onMouseClicked="#closeAction" prefHeight="27.0" prefWidth="200.0" styleClass="button:hover" stylesheets="@../../../style.css" text="Exit" />
   </children>
</VBox>
