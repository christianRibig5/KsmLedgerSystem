<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<StackPane prefHeight="300.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ksmledger.frames.FXMLDocumentController">
   <children>
      <ImageView fx:id="imageView" fitHeight="415.0" fitWidth="719.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/full_bg_blue.jpg" />
         </image></ImageView>
      <Pane prefHeight="200.0" prefWidth="200.0">
         <children>
            <Label fx:id="lblWelcome" layoutX="241.0" layoutY="176.0" prefHeight="47.0" prefWidth="217.0" text="KSM ADMIN" textFill="#d0a39b">
               <font>
                  <Font size="38.0" />
               </font>
            </Label>
            <Label fx:id="lblVersion" layoutX="652.0" layoutY="10.0" text="Ledger 1.0" textFill="#8a7d7d" />
            <Label fx:id="progress" layoutX="318.0" layoutY="229.0" prefHeight="17.0" prefWidth="105.0" text="Loading..." textFill="#f2e0e0">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font></Label>
            <ProgressBar fx:id="progressbar" disable="true" layoutY="253.0" opacity="0.23" prefHeight="10.0" prefWidth="719.0" progress="0.0" stylesheets="@../../../style.css">
               <effect>
                  <ColorAdjust />
               </effect>
               <styleClass>
                  <String fx:value="my-progress-bar" />
                  <String fx:value="red-progress-bar" />
               </styleClass></ProgressBar>
         </children>
         <effect>
            <Bloom threshold="0.41" />
         </effect>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </Pane>
   </children>
</StackPane>
