<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="510.0" prefWidth="899.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ksmledger.frames.MemberListController">
   <children>
      <Pane prefHeight="510.0" prefWidth="57.0" style="-fx-background-color: #143855;">
         <children>
            <ImageView fx:id="backButton" fitHeight="21.0" fitWidth="17.0" layoutX="17.0" layoutY="21.0" onMouseClicked="#backAction" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/arrow_back.png" />
               </image>
            </ImageView>
            <Label fx:id="closeButton" layoutX="17.0" layoutY="461.0" onMouseClicked="#closeAction" prefHeight="18.0" prefWidth="17.0" text="X" textFill="WHITE">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <TableView fx:id="table" layoutX="57.0" layoutY="12.0" prefHeight="497.0" prefWidth="843.0">
        <columns>
          <TableColumn fx:id="col_id" prefWidth="37.0" text="S/N" />
          <TableColumn fx:id="col_firstname" prefWidth="99.0" text="FIRSTNAME" />
            <TableColumn fx:id="col_lastname" prefWidth="108.0" text="LASTNAME" />
            <TableColumn fx:id="col_membership_id" prefWidth="113.0" text="MEMBERSHIP ID" />
            <TableColumn fx:id="col_email" prefWidth="190.0" text="EMAIL" />
            <TableColumn fx:id="col_phone" prefWidth="152.0" text="PHONE NUMBER" />
            <TableColumn fx:id="col_init_date" prefWidth="141.0" text="INITIATION DATE" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
